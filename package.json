    "remark-cli": "^7.0.1",
    "vuepress": "^1.2.0",
    "remark-html": "^10.0.0",
    "js-yaml": "^3.13.1"
  },
  "peerDependencies": {
    "remark-cli": "^7.0.1"
  }
}
